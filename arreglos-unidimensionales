 public static void main(String[] args) {
        //
        char [] vocal ;
        
        vocal = new char [5];
        vocal [0]= 'a';
        vocal [1]= 'e';
        vocal [2] = 'i';
        vocal [3] = 'o';
        vocal [4] = 'u';
        for (int i = 0; i < vocal.length; i++) {
            System.out.println(vocal [i]);
            
        }
        
